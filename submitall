if [ "$1" = "" ]; then
    funcfile=/dev/null
elif [  -f "$1" ]; then
    funcfile=$1
else
    funcfile=/tmp/$1
    echo $1 > $funcfile
fi
./gen_findif_all.py XXQUADRU YYQUADRU ZZQUADRU YDIPLEN $funcfile
echo "HF" > hf_availfun
cat $funcfile >> hf_availfun
make
echo "For overview of results open test_findif.html"
